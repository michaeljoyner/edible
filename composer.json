{
    "name": "michaeljoyner/edible",
    "description": "Very simple, very opinionated micro editable content management. Note that the end user cannot create new editable areas.",
    "type": "content management",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Joyner",
            "email": "joyner.michael@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^5.2",
        "symfony/yaml": "^3.0",
        "spatie/laravel-medialibrary": "^3.16"
    },
    "autoload": {
      "psr-4": {
        "Michaeljoyner\\Edible\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": ["tests/TestCase.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "illuminate/database": "^5.2",
        "illuminate/container": "^5.2"
    }
}
